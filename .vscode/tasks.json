{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "dotnet build",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "dev-fund-trakt-manager.sln"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$msCompile"
      ]
    },
    {
      "label": "dotnet test",
      "type": "process",
      "command": "dotnet",
      "args": [
        "test",
        "dev-fund-trakt-manager.sln"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "test",
      "problemMatcher": [
        "$msCompile"
      ]
    }
  ]
}
